@using ETrading.Models
@using ETrading.Areas.Wholesalers.Models
@model RegisteredWholesalersViewModel
@{
    ViewBag.title = "Registered Wholesalers";
}
<h5> @ViewBag.title</h5>
<div class="panel well">

<table class=" table table-striped table-hover" id="pending-confirmation-ratailers-table">
    <thead>
        <tr>
            <th>WholesalerName</th>
            <th>WholesalerAddress</th>
            <th>BusinessPhoneNumber</th>
            <th>BRCNumber</th>
            <th>Category</th>
            <th>Currency</th>
        </tr>
    </thead>
    <tbody>
            @foreach (var wholesaler in Model.RegisteredWholesalers)
            { 
        <tr>
            <td>@wholesaler.WholesalerName</td>
            <td>@wholesaler.WholesalerAddress</td>
            <td>@wholesaler.BusinessPhoneNumber</td>
            <td>@wholesaler.BRCNumber</td>
            <td>@wholesaler.Category</td>
            <td>@wholesaler.Currency</td>
        </tr>
            }
    </tbody>
</table>
</div>
