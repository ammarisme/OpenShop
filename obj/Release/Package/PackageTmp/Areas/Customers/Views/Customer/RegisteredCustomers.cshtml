@using ETrading.Models
@using ETrading.Areas.Customers.Models
@model RegisteredCustomersViewModel
@{
    ViewBag.title = "Registered Customers";
}
<h5> @ViewBag.title</h5>
<div class="panel well">

<table class=" table table-striped table-hover" id="customers-table">
    <thead>
        <tr>
            <th>CustomerFullName</th>
            <th>City</th>
            <th>BillingAddress</th>
        </tr>
    </thead>
    <tbody>
            @foreach (var customer in Model.RegisteredCustomers) { 
        <tr>
            <td>@customer.CustomerFullName</td>
            <td>@customer.City</td>
            <td>@customer.BillingAddress</td>
        </tr>
            }
    </tbody>
</table>
</div>
