

<!-- Wholesale Cart Modal -->
<div class="modal-content modal" id="CartModal">
    <div class="modal-header">
        <h4 class="modal-title">My Order(s)</h4>
    </div>
    <div class="modal-body">
        <div id="order-tables"></div>
    </div>
    <div class="modal-footer">
        <button type="button" class="btn btn-default btn-raised" data-dismiss="modal">Close</button>
    </div>
</div>



@* Quotation Request Modal *@
<div class="modal-content modal" id="QuoteModal">
    <div class="modal-header">
        @*<button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>*@
        <h4 class="modal-title">My Quotation(s)</h4>
    </div>
    <div class="modal-body">
        <div id="quotation-tables"></div>
    </div>
    <div class="modal-footer">
        <button type="button" class="btn btn-default btn-raised" data-dismiss="modal">Close</button>
    </div>
</div>



    <div class="modal modal-content" id="add-to-quote-modal">
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
            <h4 class="modal-title">Enter Quantity and the Price</h4>
        </div>
        <div class="modal-body">
            <form action="/" method="post" id="add-to-quote-form">
                <div class="form-group label-floating col-md-6">
                    <label class="control-label" for="Quantity">Quantity</label>
                    <input class="form-control" name="Quantity" type="number">
                </div>
                <div class="form-group label-floating col-md-6">
                    <label class="control-label" for="UnitPrice">Price</label>
                    <input class="form-control" name="UnitPrice" type="number">
                </div>
                <button type="submit" class="btn btn-primary btn-raised form-control col-md-6">Add</button>
            </form>
        </div>
        <div class="modal-footer">
            <button type="button" class="btn btn-default btn-raised form-control" data-dismiss="modal">Cancel</button>
        </div>
    </div>
