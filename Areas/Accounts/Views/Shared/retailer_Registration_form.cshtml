@model WholesaleTradingPortal.Areas.Enterprises.Models.RegisterEnterpriseViewModel

    <form action="~/Enterprises/Enterprise/Register" id="Enterprise-form" method="POST">
        <div id="name-group" class="form-group">

            <label for="name">Email</label>
            @Html.TextBoxFor(m => m.Email, new { @class = "form-control" })
        </div>
        <div id="name-group" class="form-group">
            @*Password*@
            <label for="date">Password</label>
            @Html.PasswordFor(m => m.Password, new { @class = "form-control" })
        </div>

        <div id="name-group" class="form-group">
            @*ConfirmPassword*@
            <label for="date">ConfirmPassword</label>
            @Html.PasswordFor(m => m.ConfirmPassword, new { @class = "form-control" })
        </div>

        <div id="name-group" class="form-group">
            <label for="name">Rating</label>
            @Html.TextBoxFor(m => m.Rating, new { @class = "form-control" })
        </div>

        <div id="name-group" class="form-group">
            <label for="name">EnterpriseAddress</label>
            @Html.TextBoxFor(m => m.EnterpriseAddress, new { @class = "form-control" })
        </div>

        <div id="name-group" class="form-group">
            <label for="name">BillingAddress</label>
            @Html.TextBoxFor(m => m.EnterpriseName, new { @class = "form-control" })
        </div>

        <div id="name-group" class="form-group">
            <label for="name">Category</label>
            @Html.TextBoxFor(m => m.Category, new { @class = "form-control" })
        </div>

        <div id="name-group" class="form-group">
            <label for="name">BRCNumber</label>
            @Html.TextBoxFor(m => m.BRCNumber, new { @class = "form-control" })
        </div>

        <div id="name-group" class="form-group">
            <label for="name">BusinessPhoneNumber</label>
            @Html.TextBoxFor(m => m.BusinessPhoneNumber, new { @class = "form-control" })
        </div>

        <input type="submit" class="btn btn-default" value="Register Enterprise" />
    </form>

